#include <iostream>
#include <vector>
#include <string>
using namespace std;

int main()
{
vector<std::string>destinations(3);
vector<int>miles(3);
//Index is the same for the destinations and miles vectors 
destinations[0] = "PHL";
destinations[1] = "ORL";
destinations[2] = "EWR";

miles[0] = 160;
miles[1] = 220;
miles[2] = 640;
return 0;
}
